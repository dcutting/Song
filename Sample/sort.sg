[].select(_) = []
[x|xs].select(f) when f(x) = [x|xs.select(f)]
[_|xs].select(f) = xs.select(f)

[].sort = []
[p|xs].sort = xs.lt(p) + [p] + xs.gte(p)
  l.lt(p) = l.select(|x| x < p).sort
  l.gte(p) = l.select(|x| x >= p).sort

a = [5, 1, 5, 8, 3, 56, 754, 34, 42, -33, -531, 4, 59]

out(a)
out(a.sort)
