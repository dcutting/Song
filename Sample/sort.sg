[].select(_) = []
[x|xs].select(f) when f(x) = [x|xs.select(f)]
[_|xs].select(f) = xs.select(f)

list.left(p) = list.select(|x| x < p).sort
list.right(p) = list.select(|x| x >= p).sort

[].sort = []
[p|xs].sort = xs.left(p) + [p] + xs.right(p)

a = [5, 1, 5, 8, 3, 56, 754, 34, 42, -33, -531, 4, 59]

out(a)
out(a.sort)
