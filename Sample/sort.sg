[].select(_) = []
[x|xs].select(f) when f(x) = [x|xs.select(f)]
[_|xs].select(f) = xs.select(f)

[].sort = []
[p|xs].sort = do
  left = xs.select(|x| x < p).sort
  right = xs.select(|x| x >= p).sort
  left + [p] + right
end

a = [5, 1, 5, 8, 3, 56, 754, 34, 42, -33, -531, 4, 59]

do out(a), out(a.sort) end
